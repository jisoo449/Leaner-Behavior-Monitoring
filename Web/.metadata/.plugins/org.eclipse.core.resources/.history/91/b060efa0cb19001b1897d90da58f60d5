package com.different.dashboard.service;

import java.util.List;

import org.springframework.beans.factory.annotation.Autowired;
import org.springframework.stereotype.Service;

import com.different.dashboard.dao.StudentDao;
import com.different.dashboard.dto.StudentDto;
import com.different.dashboard.form.StudentForm;

@Service
public class StudentService {
	 @Autowired
	 private StudentDao studentDao;
	 
	 public List<StudentDto> getStudentList (StudentForm studentForm) throws Exception {
		 return studentDao.getStudentList(studentForm);
	 }
	 
	
}
